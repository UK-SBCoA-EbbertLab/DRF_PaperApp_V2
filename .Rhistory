knitr::opts_chunk$set(echo = TRUE)
library(reshape2)
library(ggplot2)
library(gridExtra)
mapq_vs_camo = read.table("data/originalADSP.Illumina_OriginalADSP.T2T_CHM13_v2.0.mapq_vs_camo_annotation.bed", header=F, sep="\t", stringsAsFactors = F)
mapq_Unique = mapq_vs_camo[which(mapq_vs_camo$V10 == "."),]
intersected = mapq_vs_camo[which(mapq_vs_camo$V10 != "."),]
mapq_Unique$MapqSize = mapq_Unique$V3-mapq_Unique$V2
intersected$MapqSize = intersected$V3-intersected$V2
intersected$CamoSize = intersected$V12-intersected$V11
mapq_Unique$Type = rep("MapQUnique", nrow(mapq_Unique))
intersected$Type = rep("Intersected", nrow(intersected))
ggplot(intersected[which(intersected$V6=="protein_coding"),], aes(x=log10(size+1), y=log10(V19+1))) + geom_point()
ggplot(mapq_Unique, aes(x=log10(MapqSize+1))) + geom_density()
mapqVsCamo = rbind(mapq_Unique[,c("MapqSize","Type")], intersected[,c("MapqSize","Type")])
ggplot(mapqVsCamo, aes(x=Type, y=MapqSize)) + geom_jitter(width=.1) + geom_boxplot()
camoGenes = read.table("../DRF_PaperApp/data/Illumina100_CHM13/Updated_output_07_17_2023/originalADSP.Illumina_OriginalADSP.T2T_CHM13_v2.0.percent_camo_genes.txt", header=T, sep="\t", stringsAsFactors = F)
mapqGenes = read.table("../DRF_PaperApp/data/Illumina100_CHM13/Updated_output_07_17_2023/originalADSP.Illumina_OriginalADSP.T2T_CHM13_v2.0.percent_low_mapq_genes.txt", header=T, sep="\t", stringsAsFactors = F)
merged = merge(camoGenes, mapqGenes, by="gene_name", all=T)
ggplot(merged[which(is.na(merged$perc_total.x)),], aes(x=perc_total.y)) + geom_histogram()
